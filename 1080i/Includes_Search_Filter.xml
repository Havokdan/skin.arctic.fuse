    <include name="Search_Filter_Autocomplete_Real">
        <control type="group">
            <top>100</top>
            <height>view_pad</height>
            <control type="fixedlist" id="9098">
                <top>-40</top>
                <height>160</height>
                <width>600</width>
                <left>0</left>
                <orientation>horizontal</orientation>
                <onup>9003</onup>
                <onback>PreviousMenu</onback>
