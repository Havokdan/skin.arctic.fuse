                <visible allowhiddenfocus="true">false</visible>
                <onclick>SetFocus(9099)</onclick>
                <onclick>SetProperty(SearchTerm,$INFO[Container(9098).ListItem.Label])</onclick>
                <onclick>RunScript(script.skinvariables,set_editcontrol=9099,window_id=1105,setfocus=9099,text=$INFO[Container(9098).ListItem.Label])</onclick>
                <include>Action_UpdateSearchTerm</include>
                <orientation>vertical</orientation>
                <itemlayout width="200" height="160">
                    <control type="label">
                        <left>40</left>
                        <right>0</right>
                        <font>font_mini_black</font>
                        <align>left</align>
                        <textcolor>main_fg_30</textcolor>
                        <label>$INFO[ListItem.Label]</label>
                        <scroll>false</scroll>
                    </control>
                </itemlayout>
                <focusedlayout width="200" height="160" condition="!Control.HasFocus(9098)">
                    <control type="label">
                        <left>40</left>
                        <right>0</right>
                        <font>font_mini_black</font>
                        <align>left</align>
                        <textcolor>main_fg_30</textcolor>
                        <label>$INFO[ListItem.Label]</label>
                        <scroll>false</scroll>
                    </control>
                </focusedlayout>
                <focusedlayout width="200" height="160" condition="Control.HasFocus(9098)">
                    <control type="image">
                        <include>Texture_Highlight_Scrollbar_H</include>
                        <height>scrollbar_w</height>
                        <bottom>50</bottom>
                        <left>30</left>
                        <right>40</right>
                        <centerleft>50%</centerleft>
                    </control>
                    <control type="label">
                        <left>40</left>
                        <right>0</right>
                        <align>left</align>
                        <font>font_mini_black</font>
                        <textcolor>main_fg_90</textcolor>
                        <label>$INFO[ListItem.Label]</label>
                        <scroll>false</scroll>
                    </control>
                </focusedlayout>
                <content>plugin://plugin.program.autocompletion?info=autocomplete&amp;&amp;id=$VAR[Path_SearchTerm]&amp;&amp;limit=9</content>
                <content>plugin://script.skinvariables/?info=get_split_string&amp;infolabel=Skin.String(SearchHistory)&amp;reload=$INFO[Skin.String(SearchHistory)]</content>
            </control>
        </control>
    </include>

